  <div class="adobe-fonts-locale-switcher__label">
    Language:
  </div>

  <button
    class="adobe-fonts-locale-switcher__popover-button"
    data-id="adobe-fonts-locale-switcher__popover-button"
    ng-click="adobeFontsLocaleSwitcherCtrl.handleToggle($event)"
    aria-haspopup="true">
    {{ adobeFontsLocaleSwitcherCtrl.getLabel() }}
  </button>

  <div
    class="spectrum-Popover spectrum-Popover--dialog spectrum-Popover--top adobe-fonts-locale-switcher__popover"
    ng-class="{ 'is-open': adobeFontsLocaleSwitcherCtrl.isOpen }">
    <div class="spectrum-Dialog-content">
      <div class="adobe-fonts-locale-switcher__locale-items"
          role="listbox"
          ng-keydown="adobeFontsLocaleSwitcherCtrl.navigateLocale($event)">
        <button class="spectrum-Menu-item add-outline-dark-background remove-border adobe-fonts-locale-switcher__locale-item-button"
            ng-attr-aria-selected="{{ adobeFontsLocaleSwitcherCtrl.isSelected(locale) || undefined }}"
            ng-disabled="adobeFontsLocaleSwitcherCtrl.isButtonItemsDisabled()"
            ng-repeat="locale in adobeFontsLocaleSwitcherCtrl.locales()"
            ng-click="adobeFontsLocaleSwitcherCtrl.updateValue(locale)"
            role="option"
            tabindex="0">
          <span class="spectrum-Menu-itemLabel">
            {{ locale.label }}
          </span>

          <svg
            class="spectrum-Icon spectrum-UIIcon-CheckmarkMedium spectrum-Menu-checkmark adobe-fonts-locale-switcher__checkbox"
            aria-hidden="true"
            focusable="false"
